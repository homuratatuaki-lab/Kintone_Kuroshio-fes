{
  "manifest_version": 1,
  "version": "1.0.0",
  "type": "APP",
  "name": {
    "ja": "黒潮祭 連携プラグイン",
    "en": "Kuroshio Festival Link Plugin"
  },
  "description": {
    "ja": "団体一覧・連絡先と申請書の提出状況を同期し、一括で更新します。",
    "en": "Syncs submission status between organization list, contact app and application forms; updates in bulk."
  },
  "icon": "image/icon.png",
  "homepage_url": {"ja": "https://example.com", "en": "https://example.com"},
  "config": {
    "html": "html/config.html",
    "js": [
      "https://js.cybozu.com/jquery/3.7.1/jquery.min.js",
      "js/config.js"
    ],
    "css": ["css/config.css"]
  },
  "desktop": {
    "js": ["js/sync.js", "js/desktop.js"],
    "css": ["css/desktop.css"]
  }
}
